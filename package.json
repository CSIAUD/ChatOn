{
  "name": "chaton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && npm run prisma:generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/tailwind.css --watch",
    "migrate:dev": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema ./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^5.5.2",
    "next": "14.0.1",
    "next-auth": "^4.24.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
